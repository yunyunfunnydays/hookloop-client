    "antd": "^5.3.2",
    "autoprefixer": "10.4.14",
    "axios": "^1.4.0",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "crypto-js": "^4.1.1",
    "dayjs": "1.11.7",
    "dotenv": "^16.3.1",
    "immer": "^10.0.2",
    "js-cookie": "^3.0.5",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "next": "^13.1.1",
    "next-connect": "^1.0.0-next.4",
    "postcss": "8.4.23",
    "react": "18.2.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "18.2.0",
    "react-query": "^3.39.3",
    "react-quill": "^2.0.0",
    "react-use-websocket": "^4.3.1",
    "tailwindcss": "3.3.1",
    "ts-node": "^10.9.1"
