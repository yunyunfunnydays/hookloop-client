  "devDependencies": {
    "@commitlint/cli": "^17.6.1",
    "@commitlint/config-conventional": "^17.6.1",
    "@next/font": "^13.3.1",
    "@types/axios": "^0.14.0",
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.13",
    "@types/crypto-js": "^4.1.1",
    "@types/js-cookie": "^3.0.3",
    "@types/lodash": "^4.14.194",
    "@types/node": "^18.15.12",
    "@types/react": "^18.0.37",
    "@types/react-beautiful-dnd": "^13.1.4",
